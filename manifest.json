{
    "name": "LocalPass",
    "version": "1.0",
    "description": "Local Password Manager",
    "icons": {
      "16": "LocalPassLogo16.png",
      "48": "LocalPassLogo48.png",
      "128": "LocalPassLogo128.png"
    },
    "background": {
        "scripts": ["onPageLoad.js"],
        "persistent": false
      },
    "browser_action": {
        "default_popup": "src/popup.html"
      },
    "permissions": ["webNavigation", "activeTab", "storage", "*://*/*"],
    "manifest_version": 2
}