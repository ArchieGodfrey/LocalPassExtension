{
    "name": "LocalPass",
    "version": "1.0",
    "description": "Local Password Manager",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "page_action": {
        "default_popup": "popup.html"
      },
    "permissions": ["activeTab", "declarativeContent", "storage", "*://192.168.1.19/*"],
    "manifest_version": 2
}